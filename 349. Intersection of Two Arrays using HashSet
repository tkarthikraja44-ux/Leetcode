class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
        Set<Integer> num = new HashSet<>();
        for(int ele:nums1)
        {
            num.add(ele);
        }
        Set<Integer> num2 = new HashSet<>();
        for(int n:nums2)
        {
            if(num.contains(n))
            {
                num2.add(n);
            }
        }
        int[] res=new int[num2.size()];
        int i=0;
        for(int ele:num2)
        {
            res[i++] = ele;
        }
        return res;
    }
}
