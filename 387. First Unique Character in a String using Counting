class Solution {
    public int firstUniqChar(String s) {
        char[] word = s.toCharArray();
        for(int i=0;i<word.length;i++)
        {
            int count = 0;
            for(int j=0;j<word.length;j++)
            {
                if(i!=j&&word[i]==word[j])
                {
                    count++;
                    break;
                }
            }
            if(count==0)
            {
                return i;
            }
        }
        return -1;
    }
}
