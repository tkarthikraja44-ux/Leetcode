class Solution {
    public char findTheDifference(String s, String t) {
        StringBuilder sb = new StringBuilder();
        int []fq = new int[26];
        for(char ele:s.toCharArray())
        {
          fq[ele-'a']++;
        }
        for(char ele:t.toCharArray())
        {
            fq[ele-'a']--;
        }
        for(int i=0;i<fq.length;i++)
        {
            if(fq[i]!=0)
            {
                sb.append((char)(i+'a'));
            }
        }
        return sb.charAt(0);

        
    }
}
